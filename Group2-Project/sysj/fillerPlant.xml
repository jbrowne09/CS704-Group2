<System xmlns="http://systemjtechnology.com">
	<SubSystem Name="SS2" Local="true">
		<ClockDomain Name="FPlantCD" Class="fillerPlant">
			
			<iSignal Name="inletOn" Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10001" />
			<iSignal Name="injectorOn" Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10001" />
			<iSignal Name="valveRetract" Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10001" />
			<iSignal Name="valveExtend" Class="com.systemj.ipc.SimpleServer" IP="127.0.0.1" Port="10001" />
			
			<oSignal Name="canBottom"  To="FControllerCD.canBottom"  Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10000" />
			<oSignal Name="canTop" To="FControllerCD.canTop"  Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="10000" />			
			
			<oSignal Name="canBottomS" To="UI.canBottomS" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
			<oSignal Name="canTopS" To="UI.canTopS" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
			<oSignal Name="inletIsOnS" To="UI.inletIsOnS" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
			<oSignal Name="injectorIsOnS" To="UI.injectorIsOnS" Class="com.systemj.ipc.SimpleClient" IP="127.0.0.1" Port="20000" />
	
		</ClockDomain>
	</SubSystem>
</System>